  "bugs": {
    "url": "https://github.com/scratchfoundation/eslint-config-scratch/issues"
  },
  "homepage": "https://github.com/scratchfoundation/eslint-config-scratch#readme",
  "keywords": [
    "eslint",
    "eslintconfig",
    "scratch"
