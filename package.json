  "optionalDependencies": {
    "eslint-plugin-react": ">=7.27.0"
  },
  "peerDependencies": {
    "@babel/eslint-parser": "^7.11.0",
    "eslint": "^8.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.25.1",
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-conventional": "17.8.1",
